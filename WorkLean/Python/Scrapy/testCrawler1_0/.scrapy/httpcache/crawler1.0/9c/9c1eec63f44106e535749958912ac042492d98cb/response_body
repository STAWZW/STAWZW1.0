                url:url,
                type : 'GET',
                dataType : 'jsonp',
                //jsonp: 'jsonpCallback',
                success:function (data){
                    S_ifollow.requestSuccess(data);
                },
                error:function(){

                }
            });
        });
    }
    _shop_attention();
