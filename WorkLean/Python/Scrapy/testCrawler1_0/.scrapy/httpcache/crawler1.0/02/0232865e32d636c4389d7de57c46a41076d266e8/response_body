                                img.setAttribute('src', src);
                            }

                            if(cfg.product.ctCloth) {
                                if (!cfg.wideVersion || !cfg.compatible) {
                                    img.setAttribute('width', nsz);
                                }
                            }
                        })(document, pageConfig);
