                            var src = img.getAttribute('data-origin');
                            var nsz = 300;

                            if ((!cfg.wideVersion || !cfg.compatible) && !cfg.product.ctCloth) {
                                img.setAttribute('width', nsz);
                                /*img.setAttribute('height', nsz);*/
                                img.setAttribute('src', src.replace('s450x450', 's'+ nsz +'x' + nsz));
