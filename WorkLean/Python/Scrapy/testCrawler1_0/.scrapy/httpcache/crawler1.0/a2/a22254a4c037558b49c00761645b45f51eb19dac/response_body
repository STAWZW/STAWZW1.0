    function importHotZoneData() {
        $.each($('.hot-link'), function(index, item) {
            var pxArray = $(item).attr('px').split(',');
            $(item).css({
                left: pxArray[0] + 'px',
                top: pxArray[1]+ 'px',
                width: pxArray[2] - 2+ 'px',
                height: pxArray[3] - 2+ 'px'
            });
        });
    }
