    <script type="text/javascript">
        var masonry_pageidx = 1;
        $(document).ready(function(){

            if($("#morenewsbotton").length > 0){
                $("#addButton").empty();
                $("#addButton").append('<a id="next" href="load_chosen.jsp?"></a>');
            }
            
            function initPage(){
                $("#masonryContent").show();
                $("#masonryContent").masonry({
                    isFitWidth: true,
                    itemSelector: '.news_li',
                    singleMode: true,
                });
                var pageHeight = $(".main_lt").height();
                $(".main_rt").css({minHeight: pageHeight});
                shareNews('http://www.thepaper.cn');
            }

            var adInContent = new AdXmlDeal({
                url:"//adpai.thepaper.cn/s?",
                success:function(){
                    initPage();
                },
                error:function(){
                    initPage();
                }
            })
            
            adInContent.init();

            $('#masonryContent').infinitescroll({
                navSelector   : "#next:last",
                nextSelector  : "a#next:last",
                itemSelector  : ".news_li",
                debug   : true,
                dataType    : 'html',
                maxPage : 30,
                loading: {
                    msgText:'',
                    img: "../img/loading.gif",
                    selector: '#addButton'
                },
                finished: function(){
                    if(masonry_pageidx >= 30){
                        $("#next").remove();
                        $("#infscr-loading").hide();
                        $(window).unbind('.infscr');
                    }
                },
                pathParse: function(path,pageidx){
                    return function(pageidx){
                        var lastTime = $('#last' + masonry_pageidx).attr("lastTime");
                        masonry_pageidx = parseInt($('#last' + masonry_pageidx).attr("pageIndex"));
                        masonry_pageidx = masonry_pageidx + 1;
                        return "load_chosen.jsp?nodeids=25949&topCids=4499627,4495122,4500595,4498861,4499870,&pageidx=" + masonry_pageidx + "&lastTime=" + lastTime;
                    }
                }
            },function(newElements) {
                $('#masonryContent').masonry( 'appended', newElements, true );

                if(masonry_pageidx >= 30){
                    $("#addButton").css('display','block');
                    $("#addButton").empty();
                    $("#addButton").append('<a style="color: #01a5ec;" href="javascript:void(0);" id="morenewsbotton" onClick="morenews(this);">点击加载更多</a>');
                }

                var pageHeight = $(".main_lt").height();
                $(".main_rt").css({minHeight: pageHeight});
                shareNews('http://www.thepaper.cn');
            });

        });


        function morenews(obj){
            if(obj.disabled) return ;
            obj.disabled = true;
            var lastTime = $('#last' + masonry_pageidx).attr("lastTime");
            masonry_pageidx = parseInt($('#last' + masonry_pageidx).attr("pageIndex"));
            masonry_pageidx = masonry_pageidx + 1;
            $.ajax({
                type : "post",
                url : "load_chosen.jsp",
                data : "nodeids=25949&topCids=4499627,4495122,4500595,4498861,4499870,&pageidx=" + masonry_pageidx + "&lastTime=" + lastTime,
                timeout : 30000,
                dataType : "html",
                success : function(data) {
                    data = $.trim(data);
                    obj.disabled = false;
                    if (data == ''){
                        $("#addButton").empty();
                        $("#addButton").append('<em id="loadfinished">已经加载到底部，无法提供更多数据......</em>');
                        $("#loadfinished").fadeOut(3000);
                    }else{
                        data=data.replace(/[\r\n]/g, "");
                        var objE = document.createElement("div");
                        objE.innerHTML = data;
                        var newElements = objE.childNodes;

                        var elems = getItemElement(newElements);
                        $('#masonryContent').append(elems);
                        $('#masonryContent').masonry('appended',elems ,true);


                        var pageHeight = $(".main_lt").height();
                        $(".main_rt").css({minHeight: pageHeight});
                        shareNews('http://www.thepaper.cn');
                    }
                },
                error : function(XMLHttpRequest, textStatus, errorThrown) {
                    obj.disabled = false;
                    $("#addButton").empty();
                    $("#addButton").append('<em id="loadfinished">已经加载到底部，无法提供更多数据......</em>');
                    $("#loadfinished").fadeOut(3000);
                }
            });
        }

        
    </script>
