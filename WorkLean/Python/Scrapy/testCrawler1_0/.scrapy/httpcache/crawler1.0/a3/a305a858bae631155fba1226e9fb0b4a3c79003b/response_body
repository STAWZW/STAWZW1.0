<script>
initRightZan();
function initRightZan(){
    var zanArray = $('div[class*="ansleft_zan"]');
    for(var zani=0;zani<zanArray.length;zani++){
        var obj = zanArray.eq(zani);
        var id = obj.data("id");
        var times = obj.data("praisetimes");
        var zanNum = ZanController.check(id);
        if(zanNum){
            if(times<zanNum){
                obj.find("p").html(zanNum);
            }
            obj.find("span").addClass("zan").attr("onclick","javascript:;");
        }
    }
}
</script></div>
