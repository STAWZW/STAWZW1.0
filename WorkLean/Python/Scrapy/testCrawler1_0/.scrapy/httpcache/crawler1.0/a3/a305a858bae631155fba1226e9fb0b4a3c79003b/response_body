<script>
$(document).ready(function () {
    rightBannerInit();
});
function rightBannerError(obj){
    obj.parentNode.parentNode.parentNode.parentNode.parentNode.parentNode.parentNode.hidden = true
}
function rightBannerInit(){/*侧边栏banner*/
    var bannerArray = $(".right_banner_slide .bs_item");
    for(var i=0;i<bannerArray.length;i++){
        var box = bannerArray.eq(i);
        if(box.find("li").length>1){
            box.slide({
                titCell: "",
                mainCell: ".mr_fr ul",
                autoPage: true,
                effect: "topLoop",
                autoPlay: true,
                interTime:3000,
                vis: 1
            });
        }
    }
}
</script>

