                        function is_sort_black_list() {
              var jump_sort_list = {"6881":3,"1195":3,"10011":3,"6980":3,"12360":3};
              if(jump_sort_list['9987'] == 1 || jump_sort_list['653']==2 || jump_sort_list['655']==3) {
                return false;
