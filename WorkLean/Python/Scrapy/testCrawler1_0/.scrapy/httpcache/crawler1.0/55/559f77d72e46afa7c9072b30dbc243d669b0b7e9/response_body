    var jdwebm = document.createElement('script');
    jdwebm.type = 'text/javascript';
    jdwebm.async = true;
    jdwebm.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'h5.360buyimg.com/ws_js/jdwebm.js?v=ProDetail';
