                (function() {
            //搜本店
            $('.button01').click(function() {
                url = '//mall.jd.com/advance_search-' + 674217 + '-' + pageConfig.product.venderId + '-' + pageConfig.product.shopId + '-0-0-0-1-1-24.html';
                location.href = url + '?keyword=' + encodeURIComponent(encodeURIComponent(jQuery.trim($('#key').val())));
